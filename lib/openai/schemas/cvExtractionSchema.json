{
  "name": "cv_content_extraction",
  "strict": true,
  "schema": {
    "type": "object",
    "properties": {
      "reasoning": {
        "type": "object",
        "description": "Analyse du document AVANT extraction - remplir cette section en premier",
        "properties": {
          "document_structure": {
            "type": "string",
            "description": "Description de la structure: nombre de pages, colonnes, sections identifiées, mise en page"
          },
          "detected_language": {
            "type": "string",
            "enum": ["fr", "en", "es", "de"],
            "description": "The detected language code based on experience descriptions analysis"
          },
          "language_confirmation": {
            "type": "string",
            "description": "Explain HOW you detected the language: quote experience descriptions, list mixed-language content to translate"
          },
          "ocr_issues": {
            "type": ["string", "null"],
            "description": "Problèmes OCR détectés (caractères corrompus, texte mal encodé) et corrections à appliquer"
          },
          "key_observations": {
            "type": "string",
            "description": "Observations importantes: éléments ambigus, informations manquantes, sections inhabituelles"
          },
          "education_analysis": {
            "type": "string",
            "description": "For each item in education section: classify as education (school+degree), experience (company+job), or extras (certification/training)"
          },
          "extras_detection": {
            "type": "string",
            "description": "List hobbies, volunteering, availability, driving license, and other personal info found for extras"
          },
          "extraction_strategy": {
            "type": "string",
            "description": "Stratégie d'extraction: ordre de lecture, gestion des colonnes, regroupements à faire"
          },
          "skills_analysis": {
            "type": "string",
            "description": "For EACH skill: (1) Split compounds, (2) Translate if needed, (3) Ask: installable?→tool, language/discipline?→hard_skill, (4) Format: 'Python: LANGUAGE→hard_skill, 5yr→Advanced'"
          }
        },
        "required": ["document_structure", "detected_language", "language_confirmation", "ocr_issues", "key_observations", "education_analysis", "extras_detection", "extraction_strategy", "skills_analysis"],
        "additionalProperties": false
      },
      "header": {
        "type": "object",
        "description": "Informations personnelles et contact",
        "properties": {
          "full_name": { "type": "string", "description": "Nom complet" },
          "current_title": { "type": "string", "description": "Titre actuel" },
          "email": { "type": ["string", "null"], "description": "Email" },
          "phone": { "type": ["string", "null"], "description": "Téléphone" },
          "city": { "type": ["string", "null"], "description": "Ville" },
          "region": { "type": ["string", "null"], "description": "Région/État" },
          "country_code": { "type": ["string", "null"], "description": "Code pays ISO" },
          "links": {
            "type": "array",
            "description": "Liens (LinkedIn, GitHub, Portfolio, etc.)",
            "items": {
              "type": "object",
              "properties": {
                "label": { "type": "string", "description": "Linkedin, Github, Portfolio, Website, Other. Suivant la nature du lien (à identifier)" },
                "url": { "type": "string", "description": "URL complète" }
              },
              "required": ["label", "url"],
              "additionalProperties": false
            }
          }
        },
        "required": ["full_name", "current_title", "email", "phone", "city", "region", "country_code", "links"],
        "additionalProperties": false
      },
      "summary": {
        "type": ["object", "null"],
        "description": "Résumé professionnel",
        "properties": {
          "description": { "type": ["string", "null"], "description": "REQUIRED: Professional summary 2-3 sentences. Generate if not present in CV!" },
          "years_experience": { "type": ["integer", "null"], "description": "Années d'expérience" },
          "key_strengths": {
            "type": "array",
            "description": "Points forts principaux",
            "items": { "type": "string" }
          }
        },
        "required": ["description", "years_experience", "key_strengths"],
        "additionalProperties": false
      },
      "skills": {
        "type": "object",
        "description": "Compétences et outils",
        "properties": {
          "hard_skills": {
            "type": "array",
            "description": "Competences techniques (ex: JavaScript, Machine Learning, Data Analysis)",
            "items": {
              "type": "object",
              "properties": {
                "name": { "type": "string", "description": "Nom de la competence" },
                "proficiency": { "type": ["integer", "null"], "enum": [0, 1, 2, 3, 4, 5, null], "description": "0=Awareness, 1=Beginner, 2=Intermediate, 3=Proficient, 4=Advanced, 5=Expert" }
              },
              "required": ["name", "proficiency"],
              "additionalProperties": false
            }
          },
          "soft_skills": {
            "type": "array",
            "description": "Competences comportementales (ex: Leadership, Communication, Travail en equipe)",
            "items": { "type": "string" }
          },
          "tools": {
            "type": "array",
            "description": "Logiciels et outils (ex: Excel, SAP, Figma, VS Code)",
            "items": {
              "type": "object",
              "properties": {
                "name": { "type": "string", "description": "Nom de l'outil" },
                "proficiency": { "type": ["integer", "null"], "enum": [0, 1, 2, 3, 4, 5, null], "description": "0=Awareness, 1=Beginner, 2=Intermediate, 3=Proficient, 4=Advanced, 5=Expert" }
              },
              "required": ["name", "proficiency"],
              "additionalProperties": false
            }
          },
          "methodologies": {
            "type": "array",
            "description": "Methodologies de travail (ex: Agile, Scrum, Lean, Kanban)",
            "items": { "type": "string" }
          }
        },
        "required": ["hard_skills", "soft_skills", "tools", "methodologies"],
        "additionalProperties": false
      },
      "experience": {
        "type": "array",
        "description": "Experiences professionnelles (une entree par poste)",
        "items": {
          "type": "object",
          "properties": {
            "title": { "type": "string", "description": "Intitulé du poste" },
            "company": { "type": "string", "description": "Entreprise" },
            "department_or_client": { "type": ["string", "null"], "description": "Organizational department (Direction IT, R&D, Finance) OR client name if consultant. NOT geographic!" },
            "start_date": { "type": "string", "description": "Date début (YYYY-MM)" },
            "end_date": { "type": ["string", "null"], "description": "Date fin (YYYY-MM ou null si actuel)" },
            "city": { "type": ["string", "null"], "description": "GEOGRAPHIC city (Paris, Lyon, Berlin). NOT a department!" },
            "region": { "type": ["string", "null"], "description": "GEOGRAPHIC region/state (Île-de-France, California). NOT a department!" },
            "country_code": { "type": ["string", "null"], "description": "ISO country code (FR, US, DE)" },
            "description": { "type": ["string", "null"], "description": "Description du poste" },
            "responsibilities": {
              "type": "array",
              "description": "Responsabilités principales",
              "items": { "type": "string" }
            },
            "deliverables": {
              "type": "array",
              "description": "Résultats clés et chiffrés",
              "items": { "type": "string" }
            },
            "skills_used": {
              "type": "array",
              "description": "Compétences utilisées",
              "items": { "type": "string" }
            }
          },
          "required": ["title", "company", "department_or_client", "start_date", "end_date", "city", "region", "country_code", "description", "responsibilities", "deliverables", "skills_used"],
          "additionalProperties": false
        }
      },
      "education": {
        "type": "array",
        "description": "Formations et certifications (une entree par diplome)",
        "items": {
          "type": "object",
          "properties": {
            "institution": { "type": "string", "description": "Etablissement ou organisme" },
            "degree": { "type": ["string", "null"], "description": "Niveau d'etude (ex: Master, Licence, Diplome d'ingenieur)" },
            "field_of_study": { "type": ["string", "null"], "description": "Domaine d'etude (ex: Informatique, Commerce)" },
            "city": { "type": ["string", "null"], "description": "GEOGRAPHIC city (Paris, Lyon, Berlin). NOT a department!" },
            "region": { "type": ["string", "null"], "description": "GEOGRAPHIC region/state (Île-de-France, California). NOT a department!" },
            "country_code": { "type": ["string", "null"], "description": "ISO country code (FR, US, DE)" },
            "start_date": { "type": ["string", "null"], "description": "Date début" },
            "end_date": { "type": ["string", "null"], "description": "Date fin" }
          },
          "required": ["institution", "degree", "field_of_study", "city", "region", "country_code", "start_date", "end_date"],
          "additionalProperties": false
        }
      },
      "languages": {
        "type": "array",
        "description": "Langues parlées",
        "items": {
          "type": "object",
          "properties": {
            "name": { "type": "string", "description": "Langue" },
            "level": { "type": ["string", "null"], "description": "Niveau: A1, A2, B1, B2, C1, C2, Native" }
          },
          "required": ["name", "level"],
          "additionalProperties": false
        }
      },
      "extras": {
        "type": "array",
        "description": "ONLY: hobbies, certifications, volunteering, availability, driving license. NEVER put work experiences here - those go in 'experience' array!",
        "items": {
          "type": "object",
          "properties": {
            "name": { "type": "string", "description": "Titre" },
            "summary": { "type": ["string", "null"], "description": "Description" }
          },
          "required": ["name", "summary"],
          "additionalProperties": false
        }
      },
      "projects": {
        "type": "array",
        "description": "Projets personnels",
        "items": {
          "type": "object",
          "properties": {
            "name": { "type": "string", "description": "Nom du projet" },
            "role": { "type": ["string", "null"], "description": "Rôle" },
            "summary": { "type": ["string", "null"], "description": "Description" },
            "tech_stack": {
              "type": "array",
              "description": "Technologies utilisées",
              "items": { "type": "string" }
            },
            "start_date": { "type": ["string", "null"], "description": "Date début" },
            "end_date": { "type": ["string", "null"], "description": "Date fin" }
          },
          "required": ["name", "role", "summary", "tech_stack", "start_date", "end_date"],
          "additionalProperties": false
        }
      }
    },
    "required": ["reasoning", "header", "summary", "skills", "experience", "education", "languages", "extras", "projects"],
    "additionalProperties": false
  }
}
